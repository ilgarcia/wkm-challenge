{
  "kind": "collectionType",
  "collectionName": "persons",
  "info": {
    "singularName": "person",
    "pluralName": "persons",
    "displayName": "Person",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
