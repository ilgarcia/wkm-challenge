    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "state": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::state.state"
    },
    "city": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::city.city"
    }
  }
}
